<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quinzical.controllers.PracticeCategoryController">
   <children>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="599.0" prefWidth="441.0">
         <children>
            <Pane prefHeight="608.0" prefWidth="441.0">
               <children>
                  <Label fx:id="categoryText" alignment="BOTTOM_LEFT" layoutX="61.0" layoutY="14.0" text="Select a Category:" textAlignment="CENTER">
                     <font>
                        <Font size="36.0" />
                     </font></Label>
                  <ListView fx:id="CategoryList" layoutX="71.0" layoutY="82.0" opacity="0.8" prefHeight="406.0" prefWidth="299.0" />
                  <Button fx:id="returnBtn" layoutX="116.0" layoutY="489.0" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="39.0" prefWidth="40.0" />
                  <Button fx:id="submit" layoutX="71.0" layoutY="489.0" mnemonicParsing="false" onAction="#changeToPracticeQuestion" prefHeight="38.0" prefWidth="39.0" />
               </children></Pane>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../resources/stylesheets/QuinzicalStyle.css" />
   </stylesheets>
</StackPane>
